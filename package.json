{
  "name": "number-pyramid-generator",
  "version": "0.1.0",
  "description": "Application to generate the number pyramid and final value for a word or phrase.",
  "main": "server/development.js",
  "directories": {
    "test": "test",
    "client": "app",
    "server": "server",
    "production": "build"
  },
  "engines": {
    "node": "6.9.2",
    "npm": "3.10.9"
  },
  "scripts": {
    "clean": "rimraf build/",
    "test": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register -r babel-polyfill -r dotenv/config -r ./test/setup.js test/**/*.js",
    "start": "cross-env NODE_ENV=development nodemon -r babel-register -r babel-polyfill -r dotenv/config server/development.js",
    "prebuild": "npm run clean",
    "build:server": "webpack -p --config ./webpack.server.js --progress --color",
    "build:app": "webpack -p --config ./webpack.production.config.js --progress --color",
    "build": "cross-env NODE_ENV=production npm run build:app && npm run build:server",
    "predeploy": "npm run build",
    "lint": "eslint app/",
    "start-production": "pm2 start -i 2 --name='ActionPlan' build/production.server.bundle.js",
    "start:demo": "cross-env NODE_ENV=production node -r dotenv/config build/production.server.bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/natac13/number-pyrmid-generator.git"
  },
  "author": "Sean Campbell <sean.campbell13@gmail.com> (https://seancampbellnatac.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/natac13/number-pyrmid-generator/issues"
  },
  "homepage": "https://github.com/natac13/number-pyrmid-generator#readme",
  "devDependencies": {
    "autoprefixer": "6.3.6",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "6.0.2",
    "babel-loader": "6.2.4",
    "babel-polyfill": "6.7.4",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.24.1",
    "babel-register": "6.26.0",
    "chai": "3.5.0",
    "chai-as-promised": "5.3.0",
    "chai-immutable": "1.5.4",
    "cross-env": "1.0.7",
    "css-loader": "0.23.1",
    "dsv-loader": "2.0.0",
    "eslint": "2.8.0",
    "eslint-config-natac13": "1.3.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "0.8.5",
    "html-webpack-plugin": "2.15.0",
    "immutable-devtools": "0.0.7",
    "mocha": "5.2.0",
    "node-sass": "4.9.4",
    "nodemon": "1.9.2",
    "normalize.css": "4.1.1",
    "postcss": "6.0.14",
    "postcss-loader": "2.0.9",
    "rimraf": "2.5.2",
    "sass-loader": "6.0.6",
    "style-loader": "0.13.1",
    "supertest": "3.3.0",
    "webpack": "3.10.0",
    "webpack-dev-middleware": "2.0.3",
    "webpack-hot-middleware": "2.21.0"
  },
  "dependencies": {
    "axios": "0.9.1",
    "bluebird": "3.3.5",
    "body-parser": "1.18.3",
    "classnames": "2.2.5",
    "compression": "1.7.3",
    "dotenv": "2.0.0",
    "express": "4.16.4",
    "history": "2.0.2",
    "hover.css": "2.1.1",
    "immutability-helper": "2.1.0",
    "immutable": "3.8.1",
    "lodash": "4.17.11",
    "material-ui": "0.20.0",
    "modularscale-sass": "2.1.1",
    "moment": "2.22.2",
    "mongoose": "5.3.9",
    "morgan": "1.9.1",
    "path": "0.12.7",
    "prop-types": "15.6.0",
    "ramda": "0.25.0",
    "react": "15.4.1",
    "react-addons-css-transition-group": "15.4.1",
    "react-bootstrap": "0.31.3",
    "react-dom": "15.4.1",
    "react-fa": "4.1.2",
    "react-immutable-proptypes": "1.7.1",
    "react-redux": "4.4.5",
    "react-router": "3.0.5",
    "react-router-redux": "4.0.2",
    "react-tap-event-plugin": "2.0.1",
    "recompose": "0.17.0",
    "redux": "3.6.0",
    "redux-actions": "0.9.1",
    "redux-form": "6.5.0",
    "redux-form-material-ui": "4.3.1",
    "redux-immutable": "4.0.0",
    "redux-logger": "2.6.1",
    "redux-promise": "0.5.3",
    "serve-favicon": "2.5.0"
  }
}
